<div class="destination_show_div">
	<div class="destination_posts_div">
		<h1>{{destination.name}}</h1>
		<a id="addAPost" href="/api/destinations/{{destination._id}}/posts/new" class= "btn btn-primary">Add a Post</a>
	</div>
		
	<div class="multiple_posts">

		{{#each posts}}
			  <a href="/api/posts/{{id}}">
			<div class="single_post">
	  			<div>Interests: {{this.interests}}</div>
	  			<div>Email: {{this.email}}</div>
	  			<div class="description_content">Description: {{this.description}}</div>
		  		<!-- 	<img class="cityImg" src="{{this.photoUrl}}"> -->
			</div>
			 	</a>
	{{/each}}

	</div>
</div>


