<h1> CITIES </h1>


	{{#each destinations}}
	  <a href="/api/destinations/{{id}}">
	  	<div class="col-md-4 cities">
	  		<div class="panel panel-default">
	  			<div class="panel-body">{{this.name}}</div>
	  			<img class="cityImg" src="{{this.photoUrl}}"</div>
				</div>
	  	</div>
	  </a>
	{{/each}}
	

