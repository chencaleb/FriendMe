<h1> CITIES </h1>
<div class="container">
	{{#each destinations}}
	<a href="/api/destinations/{{id}}">
		<div class="city col-md-4">
			<div class="panel panel-default">
				<div class="panel-body">
					{{this.name}}
				</div>
	  			<img class="cityImg" src="{{this.photoUrl}}">
			</div>
	  	</div>
	 </a>
	{{/each}}
</div>

